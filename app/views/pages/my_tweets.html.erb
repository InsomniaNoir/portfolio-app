  <main role="main" class="inner cover">
    <h1 class="cover-heading">Twitter Timeline</h1>

    <div class="row">
      <%= @tweets.each do |tweet| %>
        <div class="col-md-6">
          <div class="my-tweets">
            <div class="card">
              <%= tweet %>
            </div>
          </div>
        </div>
      <% end %>
    </div>
  </main>
